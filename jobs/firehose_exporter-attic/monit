check process firehose_exporter
  with pidfile /var/vcap/sys/run/firehose_exporter-attic/firehose_exporter.pid
  start program "/var/vcap/jobs/firehose_exporter-attic/bin/firehose_exporter_ctl start"
  stop program "/var/vcap/jobs/firehose_exporter-attic/bin/firehose_exporter_ctl stop"
  group vcap
